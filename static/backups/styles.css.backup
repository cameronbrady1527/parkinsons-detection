/* ===== RESET & BASE STYLES ===== */
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

:root {
    /* Color Palette */
    --primary-color: #6366f1;
    --primary-dark: #4f46e5;
    --primary-light: #818cf8;
    --secondary-color: #10b981;
    --secondary-dark: #059669;
    --accent-color: #f59e0b;
    --danger-color: #ef4444;
    --success-color: #10b981;
    --warning-color: #f59e0b;
    
    /* Neutral Colors */
    --white: #ffffff;
    --gray-50: #f9fafb;
    --gray-100: #f3f4f6;
    --gray-200: #e5e7eb;
    --gray-300: #d1d5db;
    --gray-400: #9ca3af;
    --gray-500: #6b7280;
    --gray-600: #4b5563;
    --gray-700: #374151;
    --gray-800: #1f2937;
    --gray-900: #111827;
    
    /* Gradients */
    --gradient-primary: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    --gradient-secondary: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);
    --gradient-success: linear-gradient(135deg, #4facfe 0%, #00f2fe 100%);
    --gradient-danger: linear-gradient(135deg, #fa709a 0%, #fee140 100%);
    --gradient-dark: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    
    /* Typography */
    --font-primary: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
    --font-secondary: 'Poppins', sans-serif;
    
    /* Spacing */
    --spacing-xs: 0.25rem;
    --spacing-sm: 0.5rem;
    --spacing-md: 1rem;
    --spacing-lg: 1.5rem;
    --spacing-xl: 2rem;
    --spacing-2xl: 3rem;
    --spacing-3xl: 4rem;
    
    /* Border Radius */
    --radius-sm: 0.375rem;
    --radius-md: 0.5rem;
    --radius-lg: 0.75rem;
    --radius-xl: 1rem;
    --radius-2xl: 1.5rem;
    
    /* Shadows */
    --shadow-sm: 0 1px 2px 0 rgb(0 0 0 / 0.05);
    --shadow-md: 0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1);
    --shadow-lg: 0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1);
    --shadow-xl: 0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1);
    --shadow-2xl: 0 25px 50px -12px rgb(0 0 0 / 0.25);
    
    /* Transitions */
    --transition-fast: 0.15s ease;
    --transition-normal: 0.3s ease;
    --transition-slow: 0.5s ease;
}

body {
    font-family: var(--font-primary);
    background: var(--gradient-primary);
    min-height: 100vh;
    color: var(--gray-800);
    line-height: 1.6;
    overflow-x: hidden;
    text-align: center;
}

/* ===== BACKGROUND ANIMATION ===== */
.background-animation {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
    overflow: hidden;
}

.floating-shapes {
    position: relative;
    width: 100%;
    height: 100%;
}

.shape {
    position: absolute;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.1);
    animation: float 20s infinite linear;
}

.shape-1 {
    width: 80px;
    height: 80px;
    top: 10%;
    left: 10%;
    animation-delay: 0s;
}

.shape-2 {
    width: 120px;
    height: 120px;
    top: 60%;
    right: 10%;
    animation-delay: -5s;
}

.shape-3 {
    width: 60px;
    height: 60px;
    bottom: 20%;
    left: 20%;
    animation-delay: -10s;
}

.shape-4 {
    width: 100px;
    height: 100px;
    top: 30%;
    right: 30%;
    animation-delay: -15s;
}

@keyframes float {
    0%, 100% {
        transform: translateY(0px) rotate(0deg);
        opacity: 0.7;
    }
    50% {
        transform: translateY(-20px) rotate(180deg);
        opacity: 0.3;
    }
}

/* ===== APP CONTAINER ===== */
.app-container {
    position: relative;
    z-index: 1;
    min-height: 100vh;
    display: flex;
    flex-direction: column;
}

/* ===== HEADER ===== */
.header {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(20px);
    border-bottom: 1px solid rgba(255, 255, 255, 0.2);
    padding: var(--spacing-lg) 0;
    position: sticky;
    top: 0;
    z-index: 100;
    box-shadow: var(--shadow-lg);
}

.header-content {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 var(--spacing-xl);
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.logo-section {
    display: flex;
    align-items: center;
    gap: var(--spacing-lg);
}

.logo-icon {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 60px;
    height: 60px;
    background: var(--gradient-primary);
    border-radius: 50%;
    color: var(--white);
    font-size: 1.5rem;
    box-shadow: var(--shadow-lg);
}

.pulse-ring {
    position: absolute;
    width: 100%;
    height: 100%;
    border: 2px solid var(--primary-color);
    border-radius: 50%;
    animation: pulse 2s infinite;
}

@keyframes pulse {
    0% {
        transform: scale(1);
        opacity: 1;
    }
    100% {
        transform: scale(1.5);
        opacity: 0;
    }
}

.logo-text h1 {
    font-family: var(--font-secondary);
    font-size: 1.75rem;
    font-weight: 700;
    color: var(--gray-900);
    margin: 0;
    line-height: 1.2;
}

.subtitle {
    font-size: 0.875rem;
    color: var(--gray-600);
    font-weight: 500;
}

.header-actions {
    display: flex;
    align-items: center;
    gap: var(--spacing-lg);
}

.status-indicator {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    padding: var(--spacing-sm) var(--spacing-lg);
    background: rgba(255, 255, 255, 0.8);
    border-radius: var(--radius-xl);
    border: 1px solid var(--gray-200);
    box-shadow: var(--shadow-sm);
}

.status-dot {
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background: var(--warning-color);
    animation: pulse 2s infinite;
}

.status-dot.healthy {
    background: var(--success-color);
}

.status-dot.error {
    background: var(--danger-color);
}

.status-dot.processing,
.status-dot.loading {
    background: var(--accent-color);
    animation: pulse 1s infinite;
}

.status-text {
    font-size: 0.875rem;
    font-weight: 500;
    color: var(--gray-700);
}

.demo-btn {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    padding: var(--spacing-sm) var(--spacing-lg);
    background: var(--gradient-primary);
    color: var(--white);
    border: none;
    border-radius: var(--radius-lg);
    font-weight: 600;
    cursor: pointer;
    transition: var(--transition-normal);
    box-shadow: var(--shadow-md);
}

.demo-btn:hover:not(:disabled) {
    transform: translateY(-2px);
    box-shadow: var(--shadow-lg);
}

.demo-btn.loading {
    background: var(--gray-400);
    cursor: not-allowed;
    transform: none;
}

.demo-btn.success {
    background: var(--success-color);
    transform: translateY(-1px);
}

.demo-btn.error {
    background: var(--danger-color);
    transform: translateY(-1px);
}

.demo-btn:disabled {
    cursor: not-allowed;
    transform: none;
}

/* Demo Dropdown Styles */
.demo-dropdown {
    position: relative;
    display: inline-block;
}

.demo-arrow {
    margin-left: var(--spacing-xs);
    transition: transform var(--transition-normal);
    font-size: 0.75rem;
}

.demo-dropdown.active .demo-arrow {
    transform: rotate(180deg);
}

.demo-dropdown-content {
    display: none;
    position: absolute;
    top: 100%;
    right: 0;
    background: white;
    min-width: 320px;
    max-width: 400px;
    max-height: 70vh;
    border-radius: var(--radius-lg);
    box-shadow: var(--shadow-xl);
    border: 1px solid var(--gray-200);
    z-index: 1000;
    margin-top: var(--spacing-sm);
    overflow-y: auto;
    overflow-x: hidden;
    animation: slideDown 0.3s ease;
    scrollbar-width: thin;
    scrollbar-color: var(--gray-400) var(--gray-100);
}

.demo-dropdown.active .demo-dropdown-content {
    display: block;
}

/* Custom scrollbar for dropdown */
.demo-dropdown-content::-webkit-scrollbar {
    width: 6px;
}

.demo-dropdown-content::-webkit-scrollbar-track {
    background: var(--gray-100);
    border-radius: 3px;
}

.demo-dropdown-content::-webkit-scrollbar-thumb {
    background: var(--gray-400);
    border-radius: 3px;
}

.demo-dropdown-content::-webkit-scrollbar-thumb:hover {
    background: var(--gray-500);
}

/* ===== ENHANCED DEMO DROPDOWN STYLES ===== */
.demo-section-header {
    padding: var(--spacing-sm) var(--spacing-md);
    background: var(--gray-100);
    color: var(--text-primary);
    font-weight: 600;
    font-size: 0.9rem;
    border-bottom: 1px solid var(--border-color);
    display: flex;
    align-items: center;
    gap: var(--spacing-xs);
}

.demo-section-header.warning {
    background: #fef3c7;
    color: #92400e;
    border-color: #fed7aa;
}

.demo-section-header i {
    font-size: 0.8rem;
}

.demo-section-divider {
    height: 1px;
    background: var(--border-color);
    margin: var(--spacing-xs) 0;
}

.demo-bias-warning {
    padding: var(--spacing-sm) var(--spacing-md);
    background: #fef2f2;
    color: #dc2626;
    font-size: 0.8rem;
    font-weight: 500;
    border-left: 3px solid #dc2626;
    margin-bottom: var(--spacing-xs);
}

.demo-option {
    padding: var(--spacing-md);
    display: flex;
    align-items: flex-start;
    gap: var(--spacing-sm);
    cursor: pointer;
    transition: all 0.2s ease;
    border-bottom: 1px solid var(--border-light);
}

.demo-option:last-child {
    border-bottom: none;
}

.demo-option:hover {
    background: var(--gray-50);
    transform: translateX(2px);
}

.demo-option.realistic:hover {
    background: #f0fdf4;
    border-left: 3px solid var(--success-color);
}

.demo-option.biased:hover {
    background: #fef2f2;
    border-left: 3px solid var(--danger-color);
}

.demo-option.realistic {
    border-left: 2px solid var(--success-color);
}

.demo-option.biased {
    border-left: 2px solid var(--danger-color);
    opacity: 0.9;
}

.demo-option-icon {
    flex-shrink: 0;
    width: 32px;
    height: 32px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: var(--radius-md);
    background: var(--gray-100);
}

.demo-option.realistic .demo-option-icon {
    background: #dcfce7;
    color: var(--success-color);
}

.demo-option.biased .demo-option-icon {
    background: #fee2e2;
    color: var(--danger-color);
}

.demo-option-text {
    flex: 1;
    min-width: 0;
}

.demo-option-title {
    font-weight: 600;
    color: var(--text-primary);
    margin-bottom: 2px;
    font-size: 0.9rem;
}

.demo-option-desc {
    color: var(--text-secondary);
    font-size: 0.8rem;
    margin-bottom: 4px;
    line-height: 1.3;
}

.demo-option-stats {
    color: var(--text-tertiary);
    font-size: 0.75rem;
    font-style: italic;
}

.demo-option.realistic .demo-option-title {
    color: var(--text-primary);
}

.demo-option.biased .demo-option-title {
    color: #991b1b;
}

.demo-option.biased .demo-option-desc {
    color: #dc2626;
}

/* Mobile adjustments for new dropdown */
@media (max-width: 768px) {
    .demo-dropdown-content {
        max-height: 60vh;
        max-width: 90vw;
        right: -50px;
        overflow-y: auto;
        overflow-x: hidden;
    }
    
    .demo-option {
        padding: var(--spacing-sm);
    }
    
    .demo-option-icon {
        width: 28px;
        height: 28px;
    }
    
    .demo-section-header {
        font-size: 0.8rem;
        padding: var(--spacing-xs) var(--spacing-sm);
    }
    
    .demo-bias-warning {
        font-size: 0.75rem;
        padding: var(--spacing-xs) var(--spacing-sm);
    }
}

@keyframes slideDown {
    from {
        opacity: 0;
        transform: translateY(-10px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

/* ===== EDUCATIONAL SECTION ===== */
.educational-section {
    background: linear-gradient(135deg, #f8fafc 0%, #e2e8f0 100%);
    padding: var(--spacing-3xl) 0;
    margin-top: var(--spacing-2xl);
}

.section-header {
    text-align: center;
    margin-bottom: var(--spacing-3xl);
}

.section-title {
    font-size: 2.5rem;
    font-weight: 700;
    color: var(--gray-900);
    margin-bottom: var(--spacing-md);
    display: flex;
    align-items: center;
    justify-content: center;
    gap: var(--spacing-md);
}

.section-title i {
    color: var(--primary-color);
}

.section-subtitle {
    font-size: 1.1rem;
    color: var(--gray-600);
    max-width: 600px;
    margin: 0 auto;
}

.ide-container {
    background: #1e293b;
    border-radius: var(--radius-xl);
    box-shadow: var(--shadow-2xl);
    overflow: hidden;
    margin: 0 auto;
    max-width: 1200px;
}

/* IDE Tabs */
.ide-tabs {
    display: flex;
    background: #0f172a;
    border-bottom: 1px solid #334155;
    overflow-x: auto;
    scrollbar-width: none;
    -ms-overflow-style: none;
}

.ide-tabs::-webkit-scrollbar {
    display: none;
}

.ide-tab {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    padding: var(--spacing-md) var(--spacing-lg);
    background: #1e293b;
    color: #94a3b8;
    cursor: pointer;
    transition: all var(--transition-normal);
    border-right: 1px solid #334155;
    white-space: nowrap;
    position: relative;
    min-width: 200px;
}

.ide-tab:hover {
    background: #334155;
    color: #e2e8f0;
}

.ide-tab.active {
    background: #1e293b;
    color: #60a5fa;
    border-bottom: 2px solid #60a5fa;
}

.ide-tab i {
    font-size: 0.9rem;
}

.tab-name {
    font-family: 'Courier New', monospace;
    font-size: 0.9rem;
    font-weight: 500;
}

.tab-dot {
    width: 6px;
    height: 6px;
    border-radius: 50%;
    background: #ef4444;
    margin-left: auto;
}

.ide-tab.active .tab-dot {
    background: #10b981;
}

/* IDE Content */
.ide-content {
    background: #1e293b;
    min-height: 800px;
    position: relative;
}

.model-content {
    display: none;
    padding: 0;
    height: 100%;
}

.model-content.active {
    display: block;
}

.content-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: var(--spacing-md) var(--spacing-xl);
    background: #0f172a;
    border-bottom: 1px solid #334155;
}

.file-path {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    font-family: 'Courier New', monospace;
    font-size: 0.9rem;
    color: #94a3b8;
}

.file-path i {
    color: #60a5fa;
}

.content-meta {
    display: flex;
    gap: var(--spacing-md);
}

.accuracy, .model-type {
    padding: var(--spacing-xs) var(--spacing-sm);
    border-radius: var(--radius-sm);
    font-size: 0.8rem;
    font-weight: 600;
}

.accuracy {
    background: rgba(16, 185, 129, 0.2);
    color: #10b981;
    border: 1px solid #10b981;
}

.model-type {
    background: rgba(96, 165, 250, 0.2);
    color: #60a5fa;
    border: 1px solid #60a5fa;
}

.explanation-section {
    padding: var(--spacing-xl);
    color: #e2e8f0;
    line-height: 1.7;
}

.explanation-brief {
    background: rgba(59, 130, 246, 0.1);
    padding: var(--spacing-lg);
    border-radius: var(--radius-lg);
    margin-bottom: var(--spacing-xl);
    border-left: 4px solid #3b82f6;
}

.explanation-brief h4 {
    color: #60a5fa;
    margin-bottom: var(--spacing-md);
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
}

.explanation-brief p {
    color: #cbd5e1;
    margin: 0;
    font-size: 1.05rem;
}

.explanation-technical h4 {
    color: #f1f5f9;
    margin-bottom: var(--spacing-lg);
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    font-size: 1.3rem;
}

.code-section {
    margin-bottom: var(--spacing-xl);
}

.code-section h5 {
    color: #94a3b8;
    margin-bottom: var(--spacing-md);
    font-size: 1.1rem;
    font-weight: 600;
}

.code-block {
    background: #0f172a;
    border-radius: var(--radius-md);
    border: 1px solid #334155;
    overflow-x: auto;
    position: relative;
}

.code-block pre {
    margin: 0;
    padding: var(--spacing-lg);
    font-family: 'Courier New', 'Monaco', 'Menlo', monospace;
    font-size: 0.9rem;
    line-height: 1.5;
    color: #e2e8f0;
    overflow-x: auto;
}

.code-block code {
    background: none;
    padding: 0;
    font-family: inherit;
    font-size: inherit;
    color: inherit;
}

/* Syntax Highlighting */
.code-block .comment { color: #6b7280; }
.code-block .keyword { color: #f59e0b; font-weight: bold; }
.code-block .string { color: #10b981; }
.code-block .function { color: #60a5fa; }
.code-block .number { color: #ef4444; }
.code-block .operator { color: #94a3b8; }

.performance-note {
    background: rgba(16, 185, 129, 0.1);
    padding: var(--spacing-lg);
    border-radius: var(--radius-lg);
    border-left: 4px solid #10b981;
    margin-top: var(--spacing-xl);
}

.performance-note strong {
    color: #10b981;
}

.performance-note ul {
    margin: var(--spacing-md) 0 0 var(--spacing-lg);
    color: #cbd5e1;
}

.performance-note li {
    margin-bottom: var(--spacing-xs);
}

.performance-note li strong {
    color: #e2e8f0;
}

/* Mobile Responsiveness */
@media (max-width: 768px) {
    .section-title {
        font-size: 2rem;
        flex-direction: column;
        gap: var(--spacing-sm);
    }
    
    .ide-container {
        margin: 0 var(--spacing-md);
    }
    
    .ide-tab {
        min-width: 160px;
        padding: var(--spacing-sm) var(--spacing-md);
    }
    
    .tab-name {
        font-size: 0.8rem;
    }
    
    .content-header {
        flex-direction: column;
        gap: var(--spacing-sm);
        align-items: flex-start;
    }
    
    .explanation-section {
        padding: var(--spacing-lg);
    }
    
    .code-block pre {
        font-size: 0.8rem;
        padding: var(--spacing-md);
    }
}

@media (max-width: 480px) {
    .educational-section {
        padding: var(--spacing-xl) 0;
    }
    
    .ide-container {
        margin: 0 var(--spacing-sm);
    }
    
    .ide-tab {
        min-width: 140px;
    }
    
    .explanation-section {
        padding: var(--spacing-md);
    }
    
    .explanation-brief,
    .performance-note {
        padding: var(--spacing-md);
    }
}

/* ===== MAIN CONTENT ===== */
.main-content {
    flex: 1;
    padding: var(--spacing-2xl) 0;
}

/* ===== HERO SECTION ===== */
.hero-section {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 var(--spacing-xl);
    margin-bottom: var(--spacing-3xl);
}

.hero-content {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: var(--spacing-3xl);
    align-items: center;
}

.hero-text {
    text-align: left;
}

.hero-text h2 {
    font-family: var(--font-secondary);
    font-size: 3.5rem;
    font-weight: 700;
    color: var(--white);
    margin-bottom: var(--spacing-lg);
    line-height: 1.2;
    text-shadow: var(--shadow-lg);
}

.hero-description {
    font-size: 1.25rem;
    color: rgba(255, 255, 255, 0.9);
    margin-bottom: var(--spacing-2xl);
    line-height: 1.6;
}

.hero-actions {
    display: flex;
    gap: var(--spacing-lg);
    margin-bottom: var(--spacing-2xl);
}

.cta-btn {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    padding: var(--spacing-lg) var(--spacing-2xl);
    border: none;
    border-radius: var(--radius-xl);
    font-weight: 600;
    font-size: 1.1rem;
    cursor: pointer;
    transition: var(--transition-normal);
    text-decoration: none;
    box-shadow: var(--shadow-lg);
}

.cta-btn.primary {
    background: var(--white);
    color: var(--primary-color);
}

.cta-btn.primary:hover {
    transform: translateY(-3px);
    box-shadow: var(--shadow-xl);
}

.cta-btn.secondary {
    background: rgba(255, 255, 255, 0.2);
    color: var(--white);
    border: 2px solid rgba(255, 255, 255, 0.3);
}

.cta-btn.secondary:hover {
    background: rgba(255, 255, 255, 0.3);
    transform: translateY(-3px);
    box-shadow: var(--shadow-xl);
}

.hero-stats {
    display: flex;
    gap: var(--spacing-xl);
}

.stat-item {
    text-align: center;
    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    backdrop-filter: blur(10px);
    padding: var(--spacing-lg);
    border-radius: var(--radius-xl);
    border: 1px solid rgba(255, 255, 255, 0.3);
    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.2);
}

.stat-number {
    font-size: 1.5rem;
    font-weight: 800;
    color: var(--gray-700);
    margin-bottom: var(--spacing-xs);
    padding-right: 2px;
}

.stat-label {
    font-size: 0.875rem;
    color: rgba(255, 255, 255, 0.95);
    font-weight: 500;
}

.hero-visual {
    display: flex;
    justify-content: center;
    align-items: center;
}

.brain-animation {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 200px;
    height: 200px;
    background: rgba(255, 255, 255, 0.1);
    backdrop-filter: blur(10px);
    border-radius: 50%;
    border: 2px solid rgba(255, 255, 255, 0.2);
}

.brain-animation i {
    font-size: 4rem;
    color: var(--white);
    z-index: 2;
}

.neural-connections {
    position: absolute;
    width: 100%;
    height: 100%;
}

.connection {
    position: absolute;
    background: linear-gradient(45deg, var(--primary-light), var(--secondary-color));
    border-radius: 2px;
    animation: neural-pulse 3s infinite;
}

.connection:nth-child(1) {
    width: 60px;
    height: 2px;
    top: 20%;
    left: 10%;
    animation-delay: 0s;
}

.connection:nth-child(2) {
    width: 40px;
    height: 2px;
    top: 60%;
    right: 15%;
    animation-delay: 1s;
}

.connection:nth-child(3) {
    width: 50px;
    height: 2px;
    bottom: 30%;
    left: 20%;
    animation-delay: 2s;
}

@keyframes neural-pulse {
    0%, 100% {
        opacity: 0.3;
        transform: scaleX(0.8);
    }
    50% {
        opacity: 1;
        transform: scaleX(1);
    }
}

/* ===== UPLOAD SECTION ===== */
.upload-section {
    max-width: 800px;
    margin: 0 auto var(--spacing-3xl);
    padding: 0 var(--spacing-xl);
}

.upload-container {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(20px);
    border-radius: var(--radius-2xl);
    padding: var(--spacing-2xl);
    box-shadow: var(--shadow-2xl);
    border: 1px solid rgba(255, 255, 255, 0.2);
}

.upload-header {
    text-align: center;
    margin-bottom: var(--spacing-2xl);
}

.upload-header h3 {
    font-family: var(--font-secondary);
    font-size: 1.75rem;
    font-weight: 600;
    color: var(--gray-900);
    margin-bottom: var(--spacing-sm);
    display: flex;
    align-items: center;
    justify-content: center;
    gap: var(--spacing-sm);
}

.upload-header p {
    color: var(--gray-600);
    font-size: 1.1rem;
}

.upload-area {
    border: 3px dashed var(--primary-color);
    border-radius: var(--radius-xl);
    padding: var(--spacing-3xl) var(--spacing-2xl);
    text-align: center;
    cursor: pointer;
    transition: var(--transition-normal);
    background: rgba(99, 102, 241, 0.05);
    margin-bottom: var(--spacing-2xl);
}

.upload-area:hover {
    border-color: var(--primary-dark);
    background: rgba(99, 102, 241, 0.1);
    transform: translateY(-2px);
}

.upload-area.dragover {
    border-color: var(--success-color);
    background: rgba(16, 185, 129, 0.1);
    transform: scale(1.02);
}

.upload-icon {
    position: relative;
    display: inline-block;
    margin-bottom: var(--spacing-lg);
}

.upload-icon i {
    font-size: 3rem;
    color: var(--primary-color);
    z-index: 2;
    position: relative;
}

.upload-pulse {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 80px;
    height: 80px;
    border: 2px solid var(--primary-color);
    border-radius: 50%;
    animation: upload-pulse 2s infinite;
}

@keyframes upload-pulse {
    0% {
        transform: translate(-50%, -50%) scale(1);
        opacity: 1;
    }
    100% {
        transform: translate(-50%, -50%) scale(1.5);
        opacity: 0;
    }
}

.upload-content h4 {
    font-size: 1.5rem;
    font-weight: 600;
    color: var(--gray-900);
    margin-bottom: var(--spacing-sm);
}

.upload-content p {
    color: var(--gray-600);
    margin-bottom: var(--spacing-lg);
}

.file-requirements {
    display: flex;
    flex-direction: column;
    gap: var(--spacing-sm);
    align-items: center;
}

.requirement {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    color: var(--gray-600);
    font-size: 0.875rem;
}

.requirement i {
    color: var(--success-color);
}

.upload-actions {
    display: flex;
    gap: var(--spacing-lg);
    justify-content: center;
}

.upload-btn {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    padding: var(--spacing-lg) var(--spacing-2xl);
    border: none;
    border-radius: var(--radius-lg);
    font-weight: 600;
    cursor: pointer;
    transition: var(--transition-normal);
    position: relative;
    overflow: hidden;
}

.upload-btn.primary {
    background: var(--gradient-primary);
    color: var(--white);
    box-shadow: var(--shadow-lg);
}

.upload-btn.primary:hover:not(:disabled) {
    transform: translateY(-2px);
    box-shadow: var(--shadow-xl);
}

.upload-btn.primary:disabled {
    background: var(--gray-300);
    cursor: not-allowed;
    transform: none;
    box-shadow: none;
}

.upload-btn.loading {
    background: var(--gray-400);
    cursor: not-allowed;
    transform: none;
    box-shadow: none;
}

.upload-btn.success {
    background: var(--success-color);
    color: var(--white);
    box-shadow: var(--shadow-lg);
}

.upload-btn.error {
    background: var(--danger-color);
    color: var(--white);
    box-shadow: var(--shadow-lg);
}

.upload-btn.secondary {
    background: rgba(255, 255, 255, 0.9);
    color: var(--gray-700);
    border: 2px solid var(--gray-200);
}

.upload-btn.secondary:hover {
    background: var(--white);
    border-color: var(--primary-color);
    color: var(--primary-color);
}

.btn-loading {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.spinner-small {
    width: 20px;
    height: 20px;
    border: 2px solid rgba(255, 255, 255, 0.3);
    border-top: 2px solid var(--white);
    border-radius: 50%;
    animation: spin 1s linear infinite;
}

/* ===== RESULTS SECTION ===== */
.results-section {
    max-width: 1200px;
    margin: 0 auto var(--spacing-3xl);
    padding: 0 var(--spacing-xl);
}

.results-container {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(20px);
    border-radius: var(--radius-2xl);
    padding: var(--spacing-2xl);
    box-shadow: var(--shadow-2xl);
    border: 1px solid rgba(255, 255, 255, 0.2);
}

.results-header {
    text-align: center;
    margin-bottom: var(--spacing-2xl);
}

.results-header h3 {
    font-family: var(--font-secondary);
    font-size: 1.75rem;
    font-weight: 600;
    color: var(--gray-900);
    margin-bottom: var(--spacing-sm);
    display: flex;
    align-items: center;
    justify-content: center;
    gap: var(--spacing-sm);
}

.results-summary {
    background: var(--gray-50);
    padding: var(--spacing-lg);
    border-radius: var(--radius-lg);
    border: 1px solid var(--gray-200);
}

.summary-stats {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));
    gap: var(--spacing-lg);
}

.summary-stat {
    text-align: center;
    padding: var(--spacing-md);
    background: var(--white);
    border-radius: var(--radius-lg);
    border: 1px solid var(--gray-200);
}

.summary-number {
    font-size: 1.5rem;
    font-weight: 700;
    color: var(--primary-color);
    margin-bottom: var(--spacing-xs);
}

.summary-label {
    font-size: 0.875rem;
    color: var(--gray-600);
    font-weight: 500;
}

.confidence-text {
    font-size: 0.875rem;
    color: var(--gray-600);
    margin-top: var(--spacing-sm);
    font-weight: 500;
}

.prediction-item {
    background: var(--gray-50);
    padding: var(--spacing-md);
    border-radius: var(--radius-lg);
    border: 1px solid var(--gray-200);
    margin-bottom: var(--spacing-sm);
}

.prediction-item-header {
    margin-bottom: var(--spacing-sm);
}

.results-grid {
    display: grid;
    gap: var(--spacing-lg);
    margin-bottom: var(--spacing-2xl);
}

.prediction-card {
    background: var(--white);
    border-radius: var(--radius-xl);
    padding: var(--spacing-xl);
    border: 1px solid var(--gray-200);
    box-shadow: var(--shadow-md);
    transition: var(--transition-normal);
}

.prediction-card:hover {
    transform: translateY(-2px);
    box-shadow: var(--shadow-lg);
}

.prediction-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: var(--spacing-lg);
}

.prediction-title {
    font-weight: 600;
    color: var(--gray-900);
    font-size: 1.1rem;
}

.prediction-badge {
    padding: var(--spacing-sm) var(--spacing-lg);
    border-radius: var(--radius-xl);
    font-size: 0.875rem;
    font-weight: 600;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

.prediction-badge.positive {
    background: rgba(239, 68, 68, 0.1);
    color: var(--danger-color);
    border: 1px solid rgba(239, 68, 68, 0.2);
}

.prediction-badge.negative {
    background: rgba(16, 185, 129, 0.1);
    color: var(--success-color);
    border: 1px solid rgba(16, 185, 129, 0.2);
}

.probability-bar {
    background: var(--gray-200);
    border-radius: var(--radius-lg);
    height: 12px;
    overflow: hidden;
    margin: var(--spacing-md) 0;
}

.probability-fill {
    height: 100%;
    background: var(--gradient-success);
    transition: width var(--transition-slow);
    border-radius: var(--radius-lg);
}

.probability-fill.high-risk {
    background: var(--gradient-danger);
}

.results-actions {
    display: flex;
    gap: var(--spacing-lg);
    justify-content: center;
}

.action-btn {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
    padding: var(--spacing-md) var(--spacing-xl);
    background: var(--gray-100);
    color: var(--gray-700);
    border: 1px solid var(--gray-300);
    border-radius: var(--radius-lg);
    font-weight: 500;
    cursor: pointer;
    transition: var(--transition-normal);
}

.action-btn:hover {
    background: var(--white);
    border-color: var(--primary-color);
    color: var(--primary-color);
    transform: translateY(-1px);
}

/* ===== MODELS SECTION ===== */
.models-section {
    max-width: 1200px;
    margin: 0 auto var(--spacing-3xl);
    padding: 0 var(--spacing-xl);
}

.models-container {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(20px);
    border-radius: var(--radius-2xl);
    padding: var(--spacing-2xl);
    box-shadow: var(--shadow-2xl);
    border: 1px solid rgba(255, 255, 255, 0.2);
}

.models-header {
    text-align: center;
    margin-bottom: var(--spacing-2xl);
}

.models-header h3 {
    font-family: var(--font-secondary);
    font-size: 1.75rem;
    font-weight: 600;
    color: var(--gray-900);
    margin-bottom: var(--spacing-sm);
    display: flex;
    align-items: center;
    justify-content: center;
    gap: var(--spacing-sm);
}

.models-header p {
    color: var(--gray-600);
    font-size: 1.1rem;
}

.models-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: var(--spacing-lg);
}

.model-card {
    background: var(--white);
    border-radius: var(--radius-xl);
    padding: var(--spacing-lg);
    border: 1px solid var(--gray-200);
    box-shadow: var(--shadow-md);
    transition: var(--transition-normal);
}

.model-card:hover {
    transform: translateY(-2px);
    box-shadow: var(--shadow-lg);
}

.model-name {
    font-weight: 600;
    color: var(--gray-900);
    margin-bottom: var(--spacing-md);
    font-size: 1.1rem;
}

.model-stats {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: var(--spacing-sm);
}

.stat {
    text-align: center;
    padding: var(--spacing-sm);
    background: var(--gray-50);
    border-radius: var(--radius-lg);
    border: 1px solid var(--gray-200);
    min-width: 0;
    overflow: hidden;
}

.stat-value {
    font-size: 1.25rem;
    font-weight: 700;
    color: var(--primary-color);
    margin-bottom: var(--spacing-xs);
    line-height: 1.2;
}

.stat-label {
    font-size: 0.7rem;
    color: var(--gray-600);
    text-transform: uppercase;
    letter-spacing: 0.3px;
    font-weight: 500;
    line-height: 1.2;
    word-wrap: break-word;
    overflow-wrap: break-word;
    hyphens: auto;
}

.loading-models {
    text-align: center;
    padding: var(--spacing-2xl);
    color: var(--gray-600);
}

/* ===== FEATURES SECTION ===== */
.features-section {
    max-width: 1200px;
    margin: 0 auto var(--spacing-3xl);
    padding: 0 var(--spacing-xl);
}

.features-container {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(20px);
    border-radius: var(--radius-2xl);
    padding: var(--spacing-2xl);
    box-shadow: var(--shadow-2xl);
    border: 1px solid rgba(255, 255, 255, 0.2);
}

.features-container h3 {
    font-family: var(--font-secondary);
    font-size: 1.75rem;
    font-weight: 600;
    color: var(--gray-900);
    text-align: center;
    margin-bottom: var(--spacing-2xl);
}

.features-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: var(--spacing-lg);
}

.feature-card {
    text-align: center;
    padding: var(--spacing-xl);
    background: var(--white);
    border-radius: var(--radius-xl);
    border: 1px solid var(--gray-200);
    box-shadow: var(--shadow-md);
    transition: var(--transition-normal);
}

.feature-card:hover {
    transform: translateY(-4px);
    box-shadow: var(--shadow-xl);
}

.feature-icon {
    width: 60px;
    height: 60px;
    background: var(--gradient-primary);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto var(--spacing-lg);
    color: var(--white);
    font-size: 1.5rem;
}

.feature-card h4 {
    font-weight: 600;
    color: var(--gray-900);
    margin-bottom: var(--spacing-sm);
}

.feature-card p {
    color: var(--gray-600);
    line-height: 1.6;
}

/* ===== FOOTER ===== */
.footer {
    background: rgba(255, 255, 255, 0.95);
    backdrop-filter: blur(20px);
    border-top: 1px solid rgba(255, 255, 255, 0.2);
    padding: var(--spacing-2xl) 0 var(--spacing-lg);
    margin-top: auto;
}

.footer-content {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 var(--spacing-xl);
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: var(--spacing-2xl);
    margin-bottom: var(--spacing-xl);
}

.footer-section h4 {
    font-weight: 600;
    color: var(--gray-900);
    margin-bottom: var(--spacing-sm);
}

.footer-section p {
    color: var(--gray-600);
    line-height: 1.6;
}

.footer-bottom {
    text-align: center;
    padding-top: var(--spacing-lg);
    border-top: 1px solid var(--gray-200);
    color: var(--gray-600);
    font-size: 0.875rem;
}

/* ===== MODALS ===== */
.modal {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
    backdrop-filter: blur(5px);
    z-index: 1000;
    align-items: center;
    justify-content: center;
}

.modal.show {
    display: flex;
}

.modal-content {
    background: var(--white);
    border-radius: var(--radius-2xl);
    box-shadow: var(--shadow-2xl);
    max-width: 500px;
    width: 90%;
    overflow: hidden;
}

.modal-header {
    padding: var(--spacing-xl);
    border-bottom: 1px solid var(--gray-200);
    text-align: center;
}

.modal-header h3 {
    font-weight: 600;
    color: var(--gray-900);
    display: flex;
    align-items: center;
    justify-content: center;
    gap: var(--spacing-sm);
}

.modal-body {
    padding: var(--spacing-xl);
    text-align: center;
}

.processing-animation {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: var(--spacing-lg);
}

.brain-processing {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100px;
    height: 100px;
}

.brain-processing i {
    font-size: 2.5rem;
    color: var(--primary-color);
    z-index: 2;
}

.processing-rings {
    position: absolute;
    width: 100%;
    height: 100%;
}

.ring {
    position: absolute;
    border: 2px solid var(--primary-color);
    border-radius: 50%;
    animation: processing-ring 2s infinite;
}

.ring-1 {
    width: 60px;
    height: 60px;
    top: 20px;
    left: 20px;
    animation-delay: 0s;
}

.ring-2 {
    width: 80px;
    height: 80px;
    top: 10px;
    left: 10px;
    animation-delay: 0.5s;
}

.ring-3 {
    width: 100px;
    height: 100px;
    top: 0;
    left: 0;
    animation-delay: 1s;
}

@keyframes processing-ring {
    0% {
        transform: scale(0.8);
        opacity: 1;
    }
    100% {
        transform: scale(1.2);
        opacity: 0;
    }
}

.progress-bar {
    width: 100%;
    height: 8px;
    background: var(--gray-200);
    border-radius: var(--radius-lg);
    overflow: hidden;
}

.progress-fill {
    height: 100%;
    background: var(--gradient-primary);
    border-radius: var(--radius-lg);
    transition: width var(--transition-slow);
    width: 0%;
}

.modal-actions {
    padding: var(--spacing-xl);
    border-top: 1px solid var(--gray-200);
    display: flex;
    justify-content: center;
    gap: var(--spacing-md);
}

.btn-primary {
    padding: var(--spacing-md) var(--spacing-xl);
    background: var(--gradient-primary);
    color: var(--white);
    border: none;
    border-radius: var(--radius-lg);
    font-weight: 600;
    cursor: pointer;
    transition: var(--transition-normal);
}

.btn-primary:hover {
    transform: translateY(-2px);
    box-shadow: var(--shadow-lg);
}

.success-modal .modal-header h3 {
    color: var(--success-color);
}

.error-modal .modal-header h3 {
    color: var(--danger-color);
}

/* ===== UTILITIES ===== */
.spinner {
    width: 40px;
    height: 40px;
    border: 4px solid var(--gray-200);
    border-top: 4px solid var(--primary-color);
    border-radius: 50%;
    animation: spin 1s linear infinite;
    margin: 0 auto var(--spacing-md);
}

/* ===== NOTIFICATIONS ===== */
.notification {
    position: fixed;
    top: 20px;
    right: 20px;
    padding: var(--spacing-md) var(--spacing-lg);
    border-radius: var(--radius-lg);
    color: var(--white);
    font-weight: 500;
    z-index: 1001;
    transform: translateX(100%);
    transition: transform var(--transition-normal);
    max-width: 400px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: var(--spacing-md);
    box-shadow: var(--shadow-xl);
}

.notification-content {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
}

.notification-close {
    background: none;
    border: none;
    color: var(--white);
    cursor: pointer;
    padding: var(--spacing-xs);
    border-radius: var(--radius-sm);
    transition: var(--transition-fast);
}

.notification-close:hover {
    background: rgba(255, 255, 255, 0.2);
}

.notification.show {
    transform: translateX(0);
}

@keyframes spin {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

/* ===== RESPONSIVE DESIGN ===== */
@media (max-width: 768px) {
    .header-content {
        flex-direction: column;
        gap: var(--spacing-lg);
    }
    
    .hero-content {
        grid-template-columns: 1fr;
        text-align: center;
    }
    
    .hero-text h2 {
        font-size: 2.5rem;
    }
    
    .hero-actions {
        flex-direction: column;
        align-items: center;
    }
    
    .hero-stats {
        justify-content: center;
    }
    
    .upload-actions {
        flex-direction: column;
    }
    
    .results-actions {
        flex-direction: column;
    }
    
    .features-grid {
        grid-template-columns: 1fr;
    }
    
    .models-grid {
        grid-template-columns: 1fr;
    }
    
    .model-stats {
        grid-template-columns: repeat(3, 1fr);
        gap: var(--spacing-xs);
    }
    
    .stat {
        padding: var(--spacing-xs);
    }
    
    .stat-value {
        font-size: 1.1rem;
    }
    
    .stat-label {
        font-size: 0.65rem;
    }
    
    .footer-content {
        grid-template-columns: 1fr;
        text-align: center;
    }
}

@media (max-width: 480px) {
    .hero-text h2 {
        font-size: 2rem;
    }
    
    .upload-container {
        padding: var(--spacing-lg);
    }
    
    .upload-area {
        padding: var(--spacing-xl) var(--spacing-lg);
    }
    
    .model-stats {
        grid-template-columns: repeat(3, 1fr);
        gap: 2px;
    }
    
    .stat {
        padding: 4px;
    }
    
    .stat-value {
        font-size: 1rem;
    }
    
    .stat-label {
        font-size: 0.6rem;
        letter-spacing: 0.2px;
    }
    
    .modal-content {
        width: 95%;
        margin: var(--spacing-md);
    }
}

/* ===== PAGINATION STYLES ===== */
.pagination-container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: var(--spacing-xl);
    padding: var(--spacing-lg);
    background: var(--surface-color);
    border-radius: var(--radius-lg);
    border: 1px solid var(--border-color);
}

.pagination-info {
    color: var(--text-secondary);
    font-size: 0.875rem;
    font-weight: 500;
}

.pagination-controls {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
}

.pagination-btn {
    padding: var(--spacing-sm) var(--spacing-md);
    background: white;
    border: 1px solid var(--border-color);
    border-radius: var(--radius-md);
    color: var(--text-primary);
    font-size: 0.875rem;
    font-weight: 500;
    cursor: pointer;
    transition: all 0.2s ease;
    display: flex;
    align-items: center;
    gap: var(--spacing-xs);
}

.pagination-btn:hover {
    background: var(--gray-50);
    border-color: var(--primary-color);
    color: var(--primary-color);
    transform: translateY(-1px);
}

.pagination-btn:active {
    transform: translateY(0);
}

.pagination-btn.page-btn {
    min-width: 40px;
    height: 40px;
    padding: 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.pagination-btn.page-btn.active {
    background: var(--primary-color);
    color: white;
    border-color: var(--primary-color);
    font-weight: 600;
}

.pagination-btn.page-btn.active:hover {
    background: var(--primary-600);
    border-color: var(--primary-600);
    color: white;
}

.pagination-ellipsis {
    padding: 0 var(--spacing-sm);
    color: var(--text-secondary);
    font-weight: 500;
}

/* Mobile responsive pagination */
@media (max-width: 768px) {
    .pagination-container {
        flex-direction: column;
        gap: var(--spacing-md);
        align-items: stretch;
    }
    
    .pagination-info {
        text-align: center;
        font-size: 0.8rem;
    }
    
    .pagination-controls {
        justify-content: center;
        flex-wrap: wrap;
    }
    
    .pagination-btn {
        font-size: 0.8rem;
        padding: var(--spacing-xs) var(--spacing-sm);
    }
    
    .pagination-btn.page-btn {
        min-width: 36px;
        height: 36px;
    }
}

/* ===== CLASSIFICATION KEY STYLES ===== */
.classification-key {
    background: var(--surface-color);
    border: 1px solid var(--border-color);
    border-radius: var(--radius-lg);
    padding: var(--spacing-lg);
    margin-bottom: var(--spacing-lg);
}

.classification-key h3 {
    color: var(--text-primary);
    font-size: 1.1rem;
    margin-bottom: var(--spacing-md);
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
}

.classification-key h3 i {
    color: var(--primary-color);
}

.key-items {
    display: flex;
    flex-direction: column;
    gap: var(--spacing-md);
    margin-bottom: var(--spacing-lg);
}

.key-item {
    display: flex;
    align-items: center;
    gap: var(--spacing-md);
}

.key-badge {
    padding: var(--spacing-xs) var(--spacing-sm);
    border-radius: var(--radius-sm);
    font-size: 0.8rem;
    font-weight: 600;
    min-width: 140px;
    text-align: center;
}

.key-badge.positive {
    background: var(--danger-light);
    color: var(--danger-color);
    border: 1px solid var(--danger-color);
}

.key-badge.negative {
    background: var(--success-light);
    color: var(--success-color);
    border: 1px solid var(--success-color);
}

.confidence-indicator {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
}

.confidence-label {
    font-weight: 600;
    color: var(--text-primary);
    min-width: 140px;
}

.key-note {
    background: var(--gray-50);
    padding: var(--spacing-md);
    border-radius: var(--radius-md);
    border-left: 4px solid var(--primary-color);
    color: var(--text-secondary);
    font-size: 0.875rem;
    line-height: 1.5;
    display: flex;
    align-items: flex-start;
    gap: var(--spacing-sm);
}

.key-note i {
    color: var(--primary-color);
    margin-top: 2px;
    flex-shrink: 0;
}

/* ===== MODEL BREAKDOWN STYLES ===== */
.models-agreement {
    margin-top: var(--spacing-md);
    border-top: 1px solid var(--border-light);
    padding-top: var(--spacing-md);
}

.toggle-models-btn {
    background: var(--gray-50);
    border: 1px solid var(--border-color);
    border-radius: var(--radius-md);
    padding: var(--spacing-xs) var(--spacing-sm);
    color: var(--text-primary);
    font-size: 0.8rem;
    cursor: pointer;
    transition: all 0.2s ease;
    display: flex;
    align-items: center;
    gap: var(--spacing-xs);
    margin-top: var(--spacing-sm);
}

.toggle-models-btn:hover {
    background: var(--primary-color);
    color: white;
    border-color: var(--primary-color);
}

.model-breakdown {
    margin-top: var(--spacing-md);
    background: var(--gray-50);
    border-radius: var(--radius-md);
    padding: var(--spacing-md);
    border: 1px solid var(--border-light);
}

.model-result {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: var(--spacing-sm) 0;
    border-bottom: 1px solid var(--border-light);
}

.model-result:last-child {
    border-bottom: none;
}

.model-info {
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
}

.model-name {
    font-weight: 600;
    font-size: 0.8rem;
    color: var(--text-primary);
}

.model-prediction {
    padding: 2px 6px;
    border-radius: 4px;
    font-size: 0.7rem;
    font-weight: 600;
}

.model-prediction.positive {
    background: var(--danger-light);
    color: var(--danger-color);
}

.model-prediction.negative {
    background: var(--success-light);
    color: var(--success-color);
}

.model-probability {
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    gap: 4px;
    min-width: 120px;
}

.model-probability span {
    font-size: 0.75rem;
    color: var(--text-secondary);
}

.prob-bar {
    width: 80px;
    height: 6px;
    background: var(--gray-200);
    border-radius: 3px;
    overflow: hidden;
}

.prob-fill {
    height: 100%;
    transition: width 0.3s ease;
}

.prob-fill.positive {
    background: var(--danger-color);
}

.prob-fill.negative {
    background: var(--success-color);
}

/* ===== ENHANCED CONFIDENCE STYLES ===== */
.confidence-bar {
    width: 100%;
    height: 8px;
    background: var(--gray-200);
    border-radius: 4px;
    overflow: hidden;
    margin: var(--spacing-sm) 0;
}

.confidence-fill {
    height: 100%;
    transition: width 0.6s ease;
}

.confidence-fill.high-confidence {
    background: linear-gradient(90deg, var(--success-color), var(--success-600));
}

.confidence-fill.medium-confidence {
    background: linear-gradient(90deg, var(--warning-color), var(--warning-600));
}

.confidence-fill.low-confidence {
    background: linear-gradient(90deg, var(--danger-color), var(--danger-600));
}

/* Mobile responsive adjustments */
@media (max-width: 768px) {
    .key-items {
        gap: var(--spacing-sm);
    }
    
    .key-item {
        flex-direction: column;
        align-items: flex-start;
        gap: var(--spacing-xs);
    }
    
    .confidence-indicator {
        flex-direction: column;
        align-items: flex-start;
    }
    
    .model-result {
        flex-direction: column;
        align-items: flex-start;
        gap: var(--spacing-xs);
    }
    
    .model-probability {
        align-items: flex-start;
        width: 100%;
    }
    
    .prob-bar {
        width: 100%;
    }
}

/* ===== SCIENTIFIC ANALYSIS STYLES ===== */
.scientific-analysis {
    background: var(--surface-color);
    border: 1px solid var(--border-color);
    border-radius: var(--radius-lg);
    padding: var(--spacing-lg);
    margin-bottom: var(--spacing-lg);
}

.scientific-analysis h3 {
    color: var(--text-primary);
    font-size: 1.2rem;
    margin-bottom: var(--spacing-lg);
    display: flex;
    align-items: center;
    gap: var(--spacing-sm);
}

.scientific-analysis h3 i {
    color: var(--primary-color);
}

.analysis-sections {
    display: flex;
    flex-direction: column;
    gap: var(--spacing-lg);
}

.analysis-section {
    background: white;
    border: 1px solid var(--border-light);
    border-radius: var(--radius-md);
    padding: var(--spacing-md);
}

.analysis-section h4 {
    color: var(--text-primary);
    font-size: 1rem;
    margin-bottom: var(--spacing-md);
    border-bottom: 2px solid var(--primary-color);
    padding-bottom: var(--spacing-xs);
}

.stat-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: var(--spacing-md);
    margin-bottom: var(--spacing-md);
}

.stat-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: var(--spacing-sm);
    background: var(--gray-50);
    border-radius: var(--radius-sm);
}

.stat-label {
    font-weight: 500;
    color: var(--text-secondary);
}

.stat-value {
    font-weight: 600;
    color: var(--text-primary);
}

.analysis-note {
    padding: var(--spacing-md);
    border-radius: var(--radius-md);
    margin-top: var(--spacing-md);
    line-height: 1.6;
    display: flex;
    align-items: flex-start;
    gap: var(--spacing-sm);
}

.analysis-note.critical {
    background: #fef2f2;
    border: 1px solid #fecaca;
    color: #991b1b;
}

.analysis-note i {
    margin-top: 2px;
    flex-shrink: 0;
}

.analysis-warnings {
    display: flex;
    flex-direction: column;
    gap: var(--spacing-md);
}

.warning-item {
    display: flex;
    align-items: flex-start;
    gap: var(--spacing-sm);
    padding: var(--spacing-md);
    background: #fffbeb;
    border: 1px solid #fed7aa;
    border-radius: var(--radius-md);
    color: #92400e;
}

.warning-item i {
    color: #d97706;
    margin-top: 2px;
    flex-shrink: 0;
}

.recommendations {
    display: flex;
    flex-direction: column;
    gap: var(--spacing-sm);
}

.recommendation {
    display: flex;
    align-items: flex-start;
    gap: var(--spacing-sm);
    padding: var(--spacing-sm);
    background: #f0f9ff;
    border: 1px solid var(--primary-light);
    border-radius: var(--radius-md);
    color: var(--text-primary);
    font-size: 0.9rem;
}

.recommendation i {
    color: var(--primary-color);
    margin-top: 2px;
    flex-shrink: 0;
}

/* Mobile responsive adjustments for analysis */
@media (max-width: 768px) {
    .stat-grid {
        grid-template-columns: 1fr;
        gap: var(--spacing-sm);
    }
    
    .analysis-section {
        padding: var(--spacing-sm);
    }
    
    .analysis-note,
    .warning-item,
    .recommendation {
        flex-direction: column;
        gap: var(--spacing-xs);
    }
    
    .scientific-analysis h3 {
        font-size: 1rem;
    }
} 